<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisStore.store.repository.StoreMapper">

    <resultMap id="storeResultMap" type="com.example.mybatisStore.store.entity.Store">
        <id property="productId" column="productId"/>
        <result property="productName" column="productName"/>
        <result property="productContent" column="productContent"/>
        <result property="productImage" column="productImage" />
        <result property="prices" column="prices" />
    </resultMap>

    <!-- 게시물 등록(CREATE) -->
    <insert id="createStore" parameterType="Store">
        INSERT INTO store(productName, productContent, productImage, prices)
        VALUES (#(productName), #(productContent), #(productImage), #(prices))
    </insert>
</mapper>